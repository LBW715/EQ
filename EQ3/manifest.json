{
    "name": "Audio Equalizer",
    "version": "1.0",
    "manifest_version": 3,
    "description": "A simple graphic equalizer for adjusting audio frequencies.",
    "icons": {

    },
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "browser_action": {
      "default_title": "Audio Equalizer",
      "default_popup": "popup.html"
    }
  }
  